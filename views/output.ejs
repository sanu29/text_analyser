<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link href="public/css/style.css" rel="stylesheet" type="text/css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Libre+Baskerville:ital@1&display=swap');
     </style>
    <title>Output</title>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      
      <div class="text-title">
          TextAnalyser
      </div>
      <div class="d-flex">
          <ul class="navbar-nav mb-2 mb-lg-2">
              <li class="nav-item active">
                  <a class="nav-link" href="http://localhost:300">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="index.html">About</a>
                  </li>
          </ul>
      </div>
    </nav>
    <div class="container">
        <img src="public/css/pic1.svg" alt="analytics" class="pic">
    </div><center>
    <div class=" container-center ">
        <table class="table">
                <tr class="tabledark">
                    <td>Title</td>
                    <td>Data</td>
                </tr>
            <tr>
                    <td>Char Count</td>
                    <td><%=ch%></td>
                </tr>
                <tr>
                    <td>Word Count</td>
                    <td><%=wc%></td>
                </tr>
                <tr>
                    <td>Postitve Percentage</td>
                    <td><%=p%></td>
                </tr>
                <tr>
                    <td>Negtive percentage</td>
                    <td><%=neg%></td>
                </tr>
                <tr>
                    <td>Comparative Percentage</td>
                    <td><%=c%></td>
                </tr>
                <tr>
                    <td>Uniqueness</td>
                    <td><%=u%></td>
                </tr>
                <tr>
                    <td>Plagarism</td>
                    <td><%=pal%></td>
                </tr>
                <tr>
                    <td>Keywords</td>
                    <td>
                        <% keys.forEach(function(key){ %>
                            <%= key %>, 
                          <% }) %></td>
                </tr>
                <tr>
                    <td>Frequency</td>
                    <td>
                        <% fq.forEach(function(f){ %>
                            <%= f %>, 
                          <% }) %></td>
                </tr>
                <tr>
                    <td>Noun</td>
                    <td>
                        <% n.forEach(function(verb){ %>
                            <%= verb %>, 
                          <% }) %></td>
                         
                        </td>
                </tr>
                <tr>
                    <td>Verbs</td>
                    <td>
                        <% v.forEach(function(verb){ %>
                            <%= verb %>, 
                          <% }) %></td>
                </tr>
                <tr>
                    <td>Adjective</td>
                    <td>
                        <% adj.forEach(function(noun){ %>
                            <%= noun %>, 
                          <% }) %></td>
                </tr>
                <tr>
                    <td>Adverb</td>
                    <td>
                        <% adv.forEach(function(noun){ %>
                            <%= noun %>, 
                          <% }) %></td></td>
                </tr>
            </tbody>
        </table>

        
    </div>
<!-- Footer -->
<footer class="page-footer font-small blue">

    <!-- Copyright -->
    <div class="text-center"><small>SaniyShaikh</small>
    </div>
    <!-- Copyright -->
  
  </footer>
  <!-- Footer -->
</body>
</html>